{
  "name": "@jaredpalmer/after",
  "private": false,
  "description": "Build isomorphic Javascript applications with ease.",
  "repository": "jaredpalmer/after.js",
  "author": "Jared Palmer <jared@palmer.net>",
  "sideEffects": false,
  "version": "3.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --legacy-output --clean",
    "dev": "npm run build -- --watch",
    "clean": "rm -rf dist",
    "lint": "yarn typecheck && eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "express": ">=4",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-helmet": ">=6.0.0",
    "react-router-dom": ">=6.10.0",
    "serialize-javascript": ">=6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.12",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/serialize-javascript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testing-library": "^5.10.3",
    "express": "^4.18.2",
    "happy-dom": "^9.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.10.0",
    "serialize-javascript": "^6.0.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "babel-plugin-after": "^3.2.0"
  }
}
