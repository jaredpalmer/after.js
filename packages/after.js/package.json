{
  "name": "@jaredpalmer/after",
  "version": "3.2.0",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "babel src -d ./dist/lib --extensions .ts,.tsx --ignore '**/*.spec.tsx,**/*.test.ts' --watch",
    "build": "npm run clean && NODE_ENV=production concurrently npm:build:*",
    "build:cjs": "BABEL_ENV=cjs babel src -d ./dist/cjs --extensions .ts,.tsx --ignore '**/*.test.tsx,**/*.test.ts,**/*.d.ts' -s",
    "build:esm": "babel src -d ./dist/esm --extensions .ts,.tsx --ignore '**/*.spec.tsx,**/*.test.ts,**/*.d.ts' -s",
    "build:bundle": "tsup-node",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf ./dist/",
    "test": "jest --passWithNoTests",
    "lint": "TIMING=1 eslint src --ext .ts,.tsx --fix",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit"
  },
  "description": "Build isomorphic Javascript applications with ease.",
  "repository": "jaredpalmer/after.js",
  "author": "Jared Palmer <jared@palmer.net>",
  "sideEffects": false,
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/esm/*.js",
      "limit": "20 KB"
    }
  ],
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "babel-plugin-after": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.4",
    "@types/serialize-javascript": "^1.5.0",
    "babel-jest": "^28.1.2",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "express": "^4.17.1",
    "jest": "^28.1.2",
    "razzle-dev-utils": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "serialize-javascript": "^3.0.0",
    "size-limit": "^7.0.8",
    "ts-jest": "^28.0.5",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "express": ">=4",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-helmet": ">=6.0.0",
    "react-router-dom": ">=5.0.0",
    "serialize-javascript": ">=3.0.0"
  }
}
